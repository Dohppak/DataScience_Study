    "def count_itmes(sequence):\n",
    "    count = {}\n",
    "    for item in sequence:\n",
    "        if item in count:\n",
    "            count[item] += 1\n",
    "        else:\n",
    "            count[item] = 1\n",
    "    return count\n",
    "t = ['a','b','a','d','a']\n",
    "print(count_itmes(t))"
   ]
